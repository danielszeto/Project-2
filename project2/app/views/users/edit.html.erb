<h1>Edit Profile </h1>

<%= form_for @user do |f| %>
  <div>
    <%= f.text_field :first_name, placeholder: "First Name" %>
  </div>
  <div>
    <%= f.text_field :last_name, placeholder: "Last Name" %>
  </div>
    <div>
    <%= f.text_field :age, placeholder: "Age" %>
  </div>
    <div>
    <%= f.select :location, [['Sunset', 'sunset'],['Richmond','richmond'],['Marina','marina'],['Mission','mission'],['SOMA','soma'],['Pacific Heights','pacific heights'],['Nob Hill','nob hill']] %>
  </div>
   <div>
    <%= f.text_field :bio, placeholder: "Bio" %>
  </div>
   <div>
    <%= f.select :kind, [['Buyer','buyer'],['Seller','seller']]%>
  </div>
  <div>

  </div>
  <%= f.submit "Make Changes" %>
<% end %> 