<h1>Edit Profile </h1>

<div class="editprofile">
<%= form_for @user do |f| %>
  <div>
    First name: <%= f.text_field :first_name, placeholder: "First Name" %>
  </div>
  <div>
    Last Name: <%= f.text_field :last_name, placeholder: "Last Name" %>
  </div>
    <div>
    Age: <%= f.text_field :age, placeholder: "Age" %>
  </div>
    <div>
    Location: <%= f.select :location, [['Sunset', 'sunset'],['Richmond','richmond'],['Marina','marina'],['Mission','mission'],['SOMA','soma'],['Pacific Heights','pacific heights'],['Nob Hill','nob hill']] %>
  </div>
   <div>
    Bio: <%= f.text_field :bio, placeholder: "Bio" %>
  </div>
 
  <%= f.submit "Make Changes" %>
<% end %> 
</div>